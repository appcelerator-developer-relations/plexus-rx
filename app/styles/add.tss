    left: 0,
    width: Ti.UI.FILL,
    height: 20,
    backgroundColor: "#014780"
}

"#imageCaptureContainer": {
    top: 20,
    left: 0,
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
}

"#imageCapture": {
    top: 0,
    left: 0,
    width: Ti.UI.FILL,
    height: Ti.UI.FILL
}

"#resultView": {
    top: 0,
    left: 0,
    width: Ti.UI.FILL,
    height: Ti.UI.FILL
}

"#captureFlash": {
    top: 0,
    left: 0,
    width: Ti.UI.FILL,
    height: Ti.UI.FILL,
    backgroundColor: "#fff",
    visible: false
}

"#captureOverlay": {
    top: "15%",
    left: "15%",
    right: "15%",
    bottom: "15%",
