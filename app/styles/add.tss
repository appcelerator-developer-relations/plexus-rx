}

".captureOverlayH": {
    width: "10%",
    height: 3,
    backgroundColor: "#fff"
}

".captureOverlayV": {
    width: 3,
    height: "10%",
    backgroundColor: "#fff"
}

".captureOverlayTL": {
    top: 0,
    left: 0
}

".captureOverlayTR": {
    top: 0,
    right: 0
}

".captureOverlayLT": {
    top: 0,
    left: 0
}

".captureOverlayRT": {
    top: 0,
    right: 0
}

".captureOverlayLB": {
    bottom: 0,
    left: 0
}

".captureOverlayRB": {
    bottom: 0,
    right: 0
}

".captureOverlayBL": {
    bottom: 0,
    left: 0
}

".captureOverlayBR": {
    bottom: 0,
    right: 0
}

"#closeButton": {
    top: 35,
    right: 15,
    width: 33,
    height: 33
}

"#captureButton": {
    bottom: 30,
    width: 150,
    height: 44,
    borderRadius: 6,
    backgroundColor: "#20b572",
    color: "#fff",
    textAlign: "center",
    font: {
        fontWeight: "bold",
        fontSize: 18
    },
    opacity: 0.3,
    touchEnabled: false
}

"#observationsOverlay": {
    left: 0,
    right: 0,
