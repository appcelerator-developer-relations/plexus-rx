    height: 130,
    backgroundColor: "#cc000000",
    layout: "vertical",
    visible: false
}

".observation": {
    top: 10,
    height: 30,
    layout: "horizontal"
}

".observationIcon": {
    top: 5,
    left: 20,
    width: 20,
    height: 20,
    borderRadius: 10
}

".observationConfidence": {
    left: 20,
    width: 40,
    height: Ti.UI.FILL,
    color: "#aaa",
    textAlign: "right",
    font: {
        fontSize: 18
    }
}

".observationIdentifier": {
    left: 10,
    width: Ti.UI.FILL,
    height: Ti.UI.FILL,
    color: "#fff",
    font: {
        fontWeight: "bold",
        fontSize: 18
    }
}